{
    "name": "insights",
    "displayName": "InSights",
    "description": "Minimal code stats extension for VScode",
    "version": "0.0.3",
    "publisher": "behemehal",
    "icon": "img/icon.png",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/behemehal/InSights.git"
    },
    "keywords": [
        "vscode",
        "insights",
        "extension",
        "insights",
        "extension",
        "behemehal"
    ],
    "bugs": {
        "url": "https://github.com/behemehal/InSights/issues"
    },
    "homepage": "https://github.com/behemehal/InSights#readme",
    "engines": {
        "vscode": "^1.41.0"
    },
	"categories": [
		"Other",
		"Programming Languages"
	],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
                "command": "extension.createinsightsignore",
                "title": "Create InSights Ignore File"
            },
            {
                "command": "insights.status",
                "title": "Reload Scan"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.0.1",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    }
}
